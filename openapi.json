{
    "openapi": "3.0.3",
    "info": {
      "title": "Swagger Stray Project Bingleshop",
      "description": "This swagger for project bingleshop",
      "version": "1.0.0"
    },
    "servers": [
      {
        "url": "http://localhost:3000/api/v1"
      }
    ],
    "tags": [
      {
        "name": "Authentication",
        "description": "Authentication Login URL"
      },
      {
        "name": "Member-User",
        "description": "Group member for user URL"
      }
    ],
    "paths": {
      "/auth/login": {
        "post": {
          "tags": [
            "Authentication"
          ],
          "summary": "endpoint login into the system",
          "description": "",
          "operationId": "loginUser",
          "requestBody": {
            "description": "request body login into the system",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Login"
                }
              }
            },
            "required": true
          },
          "responses": {
            "200": {
              "description": "Login success",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/responseBody/LoginSuccess"
                  }
                }
              }
            },
            "404": {
              "description": "User or Password not match",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/responseBody/GeneralError"
                  }
                }
              }
            }
          }
        }
      },
      "/auth/register": {
        "post": {
          "tags": [
            "Authentication"
          ],
          "summary": "endpoint register into the system",
          "description": "",
          "operationId": "registerUser",
          "requestBody": {
            "description": "request body register into the system",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Register"
                }
              }
            },
            "required": true
          },
          "responses": {
            "200": {
              "description": "Register success",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/responseBody/RegisterSuccess"
                  }
                }
              }
            },
            "409": {
              "description": "User already exist",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/responseBody/GeneralError"
                  }
                }
              }
            }
          }
        }
      },
      "/member/users": {
        "get": {
          "tags": [
            "Member-User"
          ],
          "summary": "endpoint get data users by login",
          "description": "",
          "operationId": "getUser",
          "security": [
            {
               "BearerAuth": []
            }
          ],
          "responses": {
            "200": {
              "description": "success get data user",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/responseBody/UpdateUserSuccess"
                  }
                }
              }
            },
            "404": {
              "description": "User not found",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/responseBody/GeneralError"
                  }
                }
              }
            }
          }
        },
        "patch": {
          "tags": [
            "Member-User"
          ],
          "summary": "endpoint update data users",
          "description": "",
          "operationId": "updateUser",
          "security": [
            {
               "BearerAuth": []
            }
          ],
          "requestBody": {
            "description": "request body register into the system",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UserMember"
                }
              }
            },
            "required": true
          },
          "responses": {
            "200": {
              "description": "success update data user",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/responseBody/UserSuccess"
                  }
                }
              }
            },
            "404": {
              "description": "User not found",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/responseBody/GeneralError"
                  }
                }
              }
            }
          }
        }
      }
    },
    "components": {
      "schemas": {
        "Login": {
          "type": "object",
          "properties": {
            "user_name": {
              "type": "string",
              "example": "admin12345",
              "required": true
            },
            "password": {
              "type": "string",
              "example": "qwerty1234",
              "required": true
            }
          },
          "xml": {
            "name": "login"
          }
        },
        "Register": {
          "type": "object",
          "properties": {
            "role": {
              "type": "string",
              "example": "ADMIN",
              "required": true
            },
            "email": {
              "type": "string",
              "example": "akhmad.maknur@gmail.com",
              "required": true
            },
            "password": {
              "type": "string",
              "example": "password",
              "required": true
            },
            "user_name": {
              "type": "string",
              "example": "username",
              "required": true
            },
            "name": {
              "type": "string",
              "example": "your name",
              "required": true
            },
            "no_telephone": {
              "type": "string",
              "example": "your phone",
              "required": true
            },
            "alamat": {
              "type": "string",
              "example": "your address"
            }
          },
          "xml": {
            "name": "register"
          }
        },
        "UserMember": {
          "type": "object",
          "properties": {
            "email": {
              "type": "string",
              "example": "akhmad.maknur@gmail.com"
            },
            "password": {
              "type": "string",
              "example": "password"
            },
            "user_name": {
              "type": "string",
              "example": "username"
            },
            "name": {
              "type": "string",
              "example": "your name"
            },
            "no_telephone": {
              "type": "string",
              "example": "your phone"
            },
            "alamat": {
              "type": "string",
              "example": "your address"
            }
          },
          "xml": {
            "name": "register"
          }
        }       
      },
      "responseBody":{
        "LoginSuccess": {
          "type": "object",
          "properties": {
            "message": {
              "type": "string",
              "example": "Login success"
            },
            "data": {
              "type": "object",
              "properties":{
                "token": {
                "type":"string",
                  "example":"generateToken"
                },
                "refresh": {
                  "type":"string",
                  "example":"refreshToken"
                },
                "user":{
                  "type": "object",
                  "properties": {
                    "user_name": {
                      "type": "string"
                    },
                    "name": {
                      "type": "string"
                    },
                    "email": {
                      "type": "string"
                    },
                    "no_telephone": {
                      "type": "string"
                    },
                    "alamat": {
                      "type": "string",
                      "format": "nullable"
                    },
                    "role": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "xml": {
            "name": "loginSuccess"
          }
        },
        "RegisterSuccess": {
          "type": "object",
          "properties": {
            "message": {
              "type": "string",
              "example": "User registration has successful, please check your email for activation account"
            },
            "data": {
              "type": "object",
              "properties":{
                "id": {
                "type":"integer",
                  "example":1
                },
                "role_id": {
                  "type":"integer",
                  "example":1
                },
                "name": {
                  "type":"string",
                  "example":"string"
                },
                "no_telephone": {
                  "type":"string",
                  "example":"string"
                },
                "email": {
                  "type":"string",
                  "example":"string"
                },
                "alamat": {
                  "type":"string",
                  "example":"string"
                },
                "user_name": {
                  "type":"string",
                  "example":"string"
                },
                "password": {
                  "type":"string",
                  "example":"string"
                },
                "updatedAt": {
                  "type":"string",
                  "example":"string"
                },
                "createdAt": {
                  "type":"string",
                  "example":"string"
                },
                "deletedAt": {
                  "type":"string",
                  "example":"string"
                }
              }
            }
          },
          "xml": {
            "name": "registerSuccess"
          }
        },
        "UserSuccess": {
            "type": "object",
            "properties":{
              "status": {
                "type": "boolean"
              },
              "data": {
                "type": "object",
                "properties": {
                  "id": {
                    "type": "integer",
                    "format": "int32"
                },
                "name": {
                  "type": "string"
                },
                "no_telephone": {
                  "type": "string"
                },
                "email": {
                  "type": "string"
                },
                "alamat": {
                  "type": "string",
                  "format": "nullable"
                },
                "role": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "integer",
                      "format": "int32"
                    },
                    "role": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        },
        "UpdateUserSuccess": {
          "type": "object",
          "properties":{
            "status": {
              "type": "boolean"
            },
            "data": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "integer",
                  "format": "int32"
              },
              "name": {
                "type": "string"
              },
              "no_telephone": {
                "type": "string"
              },
              "email": {
                "type": "string"
              },
              "alamat": {
                "type": "string",
                "format": "nullable"
              }
            }
          }
        }
        },
        "GeneralError": {
          "type": "object",
          "properties": {
            "message": {
              "type": "string",
              "example": "string"
            }
          },
          "xml": {
            "name": "generalError"
          }
        }
      },
      "securitySchemes": {
        "BearerAuth": {
          "type": "http",
          "scheme": "bearer",
          "bearerFormat": "JWT"
        }
      }
    },
    "security": {
      "BearerAuth" : []
    }
  }